.main-container{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	padding: 0 24rpx;
}
.btn{
	color: #FFFFFF;
	border-radius: 15rpx;
}
.icon-header,.intput-container,.btn-view{
	width: 699rpx;
}
.intput-container{
	margin: 10rpx 0;
	color: #2f2f2f;
}
.intput-container .label{
	margin: 0 0 0 35rpx;
	width: 180rpx;
}
.intput-container .input-item{
	
	font-size: 28rpx;
	background-color: #f2f2f2;
	color: #2F2F2F;
	height: 90rpx;
	border-radius: 15rpx;
	margin: 35rpx 0;
	padding: 10rpx 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.intput-container .introduct-label{
	margin: 10rpx 35rpx;
	width: 180rpx;
	-webkit-align-self: flex-start;
	    -ms-flex-item-align: start;
	        align-self: flex-start;
}
.intput-container .shop-introduct{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	    -ms-flex-direction: row;
	        flex-direction: row;
	height: 250rpx;
	margin: 35rpx 0 10rpx 0;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	    -ms-flex-pack: start;
	        justify-content: flex-start;
	font-size: 28rpx;
	background-color: #f2f2f2;
	color: #2F2F2F;
	border-radius: 15rpx;
	padding: 10rpx 0;
}
.intput-container .shop-introduct .text{
	margin: 15rpx 0;
	height: 200rpx;
	-webkit-align-self: flex-start;
	    -ms-flex-item-align: start;
	        align-self: flex-start;
}
.intput-container .tips{
	font-size: 28rpx;
	color: #7f7f7f;
	margin: 0 10rpx;
}
.icon-header{
	height: 120rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-flex-wrap: wrap;
	    -ms-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.progress{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	    -ms-flex-pack: start;
	        justify-content: flex-start;
	height: 80rpx;
	width: 100%;
}
.progress-tips{
	
	display: -webkit-box;
	
	display: -webkit-flex;
	
	display: -ms-flexbox;
	
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-justify-content: space-around;
	    -ms-flex-pack: distribute;
	        justify-content: space-around;
	width: 100%;
}
.progress-tips text{
	
	font-size: 20rpx;
}
.icon-header .circle{
	font-size: 26rpx;
	font-weight: bold;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	z-index: 10;
	height: 50rpx;
	width: 50rpx;
	border-radius: 50%;
	color:#FFFFFF;
}
.circle_1{
	background-color: #FF0000;
}
.circle_2{
	background-color: #7F7F7F;
}
.circle_3{
	background-color: #7F7F7F;
}
.left-line,.middle-line_1,.middle-line_2,.right-line{
	display: -webkit-inline-box;
	display: -webkit-inline-flex;
	display: -ms-inline-flexbox;
	display: inline-flex;
	height: 5rpx;
	z-index: 1;
	background-color: #FF0000;
}
.left-line,.right-line{
	width: 15%;
}
.right-line{
	background-color: #7F7F7F;
}
.middle-line_1,.middle-line_2{
	width: 30%;
	background-color: #7F7F7F;
}
.btn-view{
	height: 100rpx;
	background-color: #4baff5;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	margin-bottom: 15rpx;
}

